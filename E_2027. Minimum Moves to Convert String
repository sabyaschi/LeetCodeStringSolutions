class Solution {
    public int minimumMoves(String s) {
        int ans = 0;
        int len = s.length();
        for(int i = 0;i < len;){
            if(s.charAt(i) == 'X'){
                i += 3;
                ans++;
            }
            else{
                i++;
            }
        }
        return ans;
    }
}
